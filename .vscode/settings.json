    "[python]": {
        "editor.formatOnSaveMode": "file",
        "editor.formatOnSave": true,
        "editor.defaultFormatter": "charliermarsh.ruff"
    },
    "python.testing.pytestEnabled": true,
    "python.analysis.extraPaths": [
        "./02_mqtt-cluster/src",
        "./02_mqtt-cluster/test",
        "./03_uns_graphdb/src",
        "./03_uns_graphdb/test",
        "./04_uns_historian/src",
